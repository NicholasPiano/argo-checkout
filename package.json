{
  "name": "argo-checkout",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/lemonmade/remote-ui.git",
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "test": "sk test",
    "lint": "sk lint",
    "type-check": "sk type-check",
